        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "     print(f\"{self.name}は{self.age}才です。\")\n",
        "\n",
        "human1 = Human(\"田中ミラベル幸一\", 89)\n",
        "human2 = Human(\"渡辺\", 29)\n",
        "\n",
        "human1.printinfo()\n",
        "human2.printinfo()"
